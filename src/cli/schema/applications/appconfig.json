{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "file:appconfig.json",
  "title": "AppConfig",
  "description": "QNE AppConfig object",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer",
      "readOnly": true
    },
    "url": {
      "type": "string",
      "format": "uri-reference",
      "readOnly": true
    },
    "app_version": {
      "type": "string",
      "format": "uri-reference"
    },
    "network": {
      "$ref": "file:../networks/network_asset.json"
    },
    "application": {
      "type": "array",
      "items": {
        "$ref": "file:../networks/roletemplate.json"
      }
    },
    "multi_round": {
      "type": "boolean"
    }
  },
  "required": [
    "network",
    "application"
  ],
  "additionalProperties": false
}
